{
  "name": "Declutter browser history",
  "version": "1.0",
  "description": "Blocks selected URLs from showing up in history.",
  "permissions": [
    "history",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "icons": {
    "96": "warehouse-96.png",
    "128": "warehouse-128.png"
  },
  "version": "1.0"
}
